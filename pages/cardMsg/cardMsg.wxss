.page{
  background-color: #f2f2f2;
}
.matchWarp{
  width: 100%;
  /* height: 100rpx; */
  background-color: #fff;
  padding: 20rpx 0;
}
.tips{
  width: 100%;
  margin: 0 auto;
  font-size: 30rpx;
  border-top: 2rpx solid #D7D7D7;
  line-height: 50rpx;
  text-indent: 2.5%;
}
.matchBox{
  width: 95%;
  /* height: 100rpx; */
  display: flex;
  align-items: center;
  margin: 0 auto;
}
.mlogo{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-right: 15rpx;
}
.mlogo >image{
  width: 100rpx;
  height: 100rpx;
}

.cardList{
  width: 85%;
  height: 160rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-bottom: 30rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.c1{
  margin-top: 30rpx;
}
.cL{
  float: left;
  width: 160rpx;
  height: 160rpx;
  background-color: #16C95D;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 48rpx;
}

.cL >image{
  width: 160rpx;
  height: 160rpx;
}
.cR{
  width: calc(100% - 200rpx);
  height: 160rpx;
  margin: 0 20rpx;
  float: left;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cardMsg{
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: center;
}
.x{
  font-size: 30rpx;
  color: #666666;
}

.myPic{
  width: 100rpx;
  height: 100rpx;
  background-color: #16C95D;
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  overflow: hidden;
}
.myPic >image{
  width: 100rpx;
  height: 100rpx;
}
.nohas>image{
  width: 50rpx;
  height: 50rpx;
}

/* 查看大图 */
.alert{
  width: 100%;
  height: 100%;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 100;
  background-color: rgba(0, 0, 0, 0.3);
  display: flex;
  justify-content: center;
  align-items: center;
}
.alertCon{
  width: 80%;
  height: 770rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.imgBox,.imgBox>image{
  width: 100%;
  height: 700rpx;
}
.closeBtn{
  margin-top: 20rpx;
  width: 50rpx;
  height: 50rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.closeBtn>image{
  width: 50rpx;
  height: 50rpx;
}