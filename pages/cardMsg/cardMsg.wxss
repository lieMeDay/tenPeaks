.page{
  background-color: #f2f2f2;
}
.matchWarp{
  width: 100%;
  height: 100rpx;
  background-color: #fff;
  padding: 20rpx 0;
}
.matchBox{
  width: 95%;
  height: 100rpx;
  display: flex;
  align-items: center;
}
.mlogo{
  width: 100rpx;
  height: 100rpx;
  border-radius: 10rpx;
  overflow: hidden;
  margin-right: 15rpx;
}
.mlogo >image{
  width: 100rpx;
  height: 100rpx;
}

.cardList{
  width: 85%;
  height: 160rpx;
  background-color: #fff;
  margin: 0 auto;
  margin-bottom: 30rpx;
  border-radius: 20rpx;
  overflow: hidden;
}
.c1{
  margin-top: 30rpx;
}
.cL{
  float: left;
  width: 160rpx;
  height: 160rpx;
  background-color: #16C95D;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 48rpx;
}
.cR{
  width: calc(100% - 200rpx);
  height: 160rpx;
  margin: 0 20rpx;
  float: left;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.cardMsg{
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  justify-content: center;
}
.x{
  font-size: 30rpx;
  color: #666666;
}

.myPic{
  width: 100rpx;
  height: 100rpx;
  background-color: #16C95D;
  border-radius: 15rpx;
  display: flex;
  align-items: center;
  justify-content: center;
}
.myPic >image{
  width: 100rpx;
  height: 100rpx;
}
.nohas>image{
  width: 50rpx;
  height: 50rpx;
}