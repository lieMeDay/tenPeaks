/* pages/match/match.wxss */
.matchLogo{
  width: 100%;
  height: 400rpx;
}
.matchLogo>image{
  width: 100%;
  height: 100%;
}
.matchWrap{
  width: 95%;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 20rpx;
  padding-bottom: 100rpx;
}
.mt{
  font-size: 40rpx;
  font-weight: 500;
}
.mc>view{
  margin-bottom: 10rpx;
}
.ric .inPimg{
  width: 100%;
  height: 100%;
}
.ric .ql-align-center{
  text-align: center;
}
.btnBox{
  width: 100%;
  height: 100rpx;
  position: fixed;
  bottom: 0;
  left: 0;
   background-color: #fff;
   display: flex;
   justify-content: center;
   align-items: center;
}
.goBtn{
  width: 80% !important;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  background-color: #16C95D;
  border-radius: 15rpx;
  color: #fff;
  font-weight: normal;
}