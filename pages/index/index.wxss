.page{
  background-color: #022d95;
}
.titWrap{
  height: 100rpx;
  line-height: 100rpx;
  /* background-image: linear-gradient(to right , #111a3b, #a5196f); */
  background-image: linear-gradient(to left, #a5196f, #111a3b);
}
.titBox{
  width: 95%;
  overflow: hidden;
  margin: 0 auto;
  color: #fff;
  display: flex;
  justify-content: space-between;
}
.listWrap{
  width: 90%;
  overflow: hidden;
  margin: 0 auto;
  margin-top: 20rpx;
}
.listBox{
  width: 100%;
  height: 150rpx;
  color: #fff;
  position: relative;
  border-radius: 16rpx;
  margin-bottom: 20rpx;
  overflow: hidden;
}
.listBox>image{
  width: 100%;
  height: 150rpx;
  position: absolute;
  top: 0;
  left: 0;
}
.listShadow{
  width: 100%;
  height: 150rpx;
  background-color: rgba(0, 0, 0, 0.2);
  position: relative;
  z-index: 10;
}
.listCon{
  width: 90%;
  height: 150rpx;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.listCon>view{
  letter-spacing: 1.5;
  font-size: 40rpx;
}
.keyLock,.keyLock>image{
  width: 120rpx;
  height: 120rpx;
}
